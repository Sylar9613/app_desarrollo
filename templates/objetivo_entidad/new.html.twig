{% extends 'base.html.twig' %}

{% block title %}Asociar Objetivo por entidad{% endblock %}

{% block body %}
    <br/>
    <!-- Breadcrumbs-->
    <ol class="breadcrumb">
        <li class="breadcrumb-item">{{ routes[0] }}</li>
        <li class="breadcrumb-item">
            <a class="normal" href="{{ path('entidad_index') }}">{{ routes[1] }}</a>
        </li>
        <li class="breadcrumb-item activo">{{ routes[2] }}</li>
    </ol>
    <h1>Asociar <b style="color: #4e555b"><i>{{ objetivo.nombre }}</i></b> con <b style="color: #4e555b"><i>{{ entidad.nombre }}</i></b></h1>
    {{ include('admin/errors.html.twig') }}
    {{ include('objetivo_entidad/_form.html.twig') }}
    <button style="margin-top: 8px;" onclick="goBack()" class="btn btn-secondary btn-block text-white">Atr&aacute;s</button>
{% endblock %}